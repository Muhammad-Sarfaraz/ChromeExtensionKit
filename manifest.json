{
  "manifest_version": 3,
  "name": "Go to Fun",
  "version": "1.0",
  "description": "Redirects to fun",
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
    }
  }
}
